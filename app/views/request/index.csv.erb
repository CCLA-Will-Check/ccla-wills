<%- headers = ['Applicant', 'Email', 'Telephone', 'LSUC', 'Testator First Name', 'Testator Last Name', 'Alt Name', 'DOB', 'City', 'Province', 'Last Resided In' ] -%>
<%= CSV.generate_line headers %>
<%- @requests.each do |request| -%>
  <%= CSV.generate_line([request.applicant,
                         request.email,
                         request.telephone,
                         request.lsuc,
                         request.testator_first,
                         request.testator_last,
                         request.testator_alt,
                         request.testator_dob,
                         request.testator_city,
                         request.testator_province,
                         request.testator_last_resident,
                         request.status]) -%>
<%- end -%>
