<table class="table"/
<tr><th>Name</th><th>Alt. Last Name</th><th>Date of Birth</th><th>Birth City</th><th>Birth Province</th><th>Birth Country</th><th>Last Resided In</th><th>Firm</th></tr>
<% for will in @wills do %>
  <tr>
    <td><%= will.first_name %> <%= will.last_name %></td>
    <td><%= will.dob %></td>
    <td><%= will.alt_last_name %></td>
    <td><%= will.city_of_birth %></td>
    <td><%= will.province_of_birth %></td>
    <td><%= will.country_of_birth %></td>
    <td><%= will.last_resided %></td>
    <td>Lawyer/Firm Information</td>
    <td><%= will.lawyer_name %></td>
    <td><%= will.firm_name %></td>
    <td><%= will.lsuc %></td>
    <td><%= will.telephone %></td></tr>
<% end %>
</table>
